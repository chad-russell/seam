(struct point 
    (x i64)
    (y i64))

(struct line
    (start (ptr point))
    (end (ptr point)))

(fn main () i64
    (let p point uninit)
    (set (field p x) 5)

    (let ppx (ptr i64) (ref (field p x)))

    (let l line uninit)
    (set (field l start) (ref p))

    (let ppp (ptr (ptr point)) (ref (field l start)))

    (set (field (load (load ppp)) x) 121)

    (return (load ppx)))
